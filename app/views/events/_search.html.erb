<ul class="right">
  <li class="has-form">
    <div class="row collapse">
      <%= form_tag(events_path, method: 'get') do %>
        <div class="large-8 small-9 columns">
          <%= text_field_tag :search, params[:search], placeholder: 'Event name or state' %>
        </div>
        <div class="large-4 small-3 columns">
          <%= submit_tag "Search", class: "button [radius round]" %>
        </div>
      <% end %>
    </div>
  </li>
</ul>
