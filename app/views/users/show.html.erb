<h3>
<%="#{@user.first_name} #{@user.last_name}"%>
</h3>

<ul>
  <li>
    Email: <%=@user.email%>
  </li>

  <li>
    Address: <%="#{@user.address} #{@user.city} #{@user.state}" %>
  </li>

  <%if @event.exists? %>
    Events
    <%=@event.each do |e|%>

       <li>
        <%=e.title%>
      </li>

      <li>
        <%=e.description%>
      </li>

      <li>
        <%=e.start_time%>
      </li>

      <%if e.end_time%>
        <li>
          <%=e.end_time%>
        </li>
      <%end%>

      <li>
        <%=e.guest_count%>
      </li>

      <li>
        <%=e.budget%>
      </li>

      <li>
        <%="#{e.address} #{e.city} #{e.state}"%>
      </li>
    <%end%>
  <% end %>



